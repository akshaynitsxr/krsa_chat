extends layout

block head_scripts
  link(href="/stylesheets/dashboard.css",rel="stylesheet")
  script(src='/javascripts/dashboard.js',type="text/javascript")
  script(src="/socket.io/socket.io.js")


block content
  .container
    .row
      .col-lg-6.justify-content-center
        p
        .chat
          .chat-history
            ul.chat-ul
        form#input-msg
          .row
            .col-sm-6.no-gutters
              input.form-control#msg(type='text', placeholder='Enter your message', autocomplete="off")
            .col-sm-6.no-gutters
              button.btn.btn-default.msg(type='submit') Send

